{
    "name": "searchtree",
    "version": "1.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Libraries with operations to strongly encapsulated non-deterministic computations as a search tree or list",
    "category": [ "Programming" ],
    "dependencies": {
        "base"    : ">= 1.0.0, < 2.0.0",
        "random"  : ">= 0.0.1"
    },
    "exportedModules": [ "Control.SearchTree", "Control.SearchTree.Generators",
                         "Control.SearchTree.Traversal",
                         "Control.AllSolutions", "Control.Findall" ],
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0",
        "kics2": ">= 2.0.0"
    },
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/searchtree.git",
        "tag": "$version"
    }
}
